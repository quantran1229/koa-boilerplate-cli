{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "max-len": "off",
        "require-jsdoc": 0,
        "valid-jsdoc": "off",
        "new-cap": "off",
        "prettier/prettier": ["warn"]
    }
}